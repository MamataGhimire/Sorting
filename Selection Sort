#include <iostream>
using namespace std;
int main() {
  int num[7]={9,0,7,8,4,1,2};
  for(int i=0;i<=5 ;i++)
  {
      int mini=i;
      for(int j=i;j<7;j++)
   {
      if(num[j]<num[mini])
      {
       mini=j;
      }
     
     }
   int swap=num[mini];
      num[mini]=num[i];
      num[i]=swap;
}

 for(int i=0;i<7;i++)
 {
     cout<<num[i];
 }
    return 0;
}
